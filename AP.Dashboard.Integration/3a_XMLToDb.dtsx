                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @Mixnummer bigint, @Stuknummer varchar(10)
set @Mixnummer = ?
set @Stuknummer = ?

if not exists (select * from AutoSamenstelling where Mixnummer = @Mixnummer and Stuknummer = @Stuknummer)
begin
insert into AutoSamenstelling(Mixnummer,Stuknummer,Aantal)
values (@Mixnummer,@Stuknummer,1)
end
else
begin
update AutoSamenstelling set Aantal = Aantal +1
where Mixnummer = @Mixnummer and Stuknummer = @Stuknummer
end</property>
